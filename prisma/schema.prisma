generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Josaa2025 {
  sno                 Decimal  @id(map: "josaa2025_pkey") @db.Decimal
  round               Decimal? @db.Decimal
  institute           String?
  academicProgramName String?
  quota               String?
  seatType            String?
  gender              String?
  openingRank         String?
  closingRank         String?
  priority            Int?
  type                String?
  nirf                String?

  @@index([institute])
}

model all {
  sno       Decimal? @db.Decimal
  institute String   @id
  type      String?

  @@index([institute])
}
